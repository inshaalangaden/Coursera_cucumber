<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Coursera POM Suite" parallel="tests" thread-count="3">
    <test name="EdgeTests">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="com.coursera.runners.TestNGTestRunner"/>
        </classes>
    </test>

    <test name="ChromeTests">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.coursera.runners.TestNGTestRunner"/>
        </classes>
    </test>
</suite>